  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead",
          "useBuiltIns": "usage",
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "react-hot-loader/babel",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": [
        "eslint --fix",
        "tslint --fix",
        "prettier --write --no-semi --single-quote --print-width 160 --jsx-bracket-same-line true",
        "git add"
      ]
    },
    "ignore": [
      "**/*.min.js"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "gulp": "gulp",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(styled-)).*$/' -a"
  }
