  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "modules": false,
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "react-hot-loader/babel",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*.min).{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --no-semi --single-quote --print-width 160 --jsx-bracket-same-line true",
      "git add"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(@types/styled-components|@typescript-eslint)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
