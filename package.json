  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "modules": false,
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "react-hot-loader/babel",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "resource/**/!(*.min).{ts,tsx,js}": "eslintã€€--fix --max-warnings 0",
    "webpack/*.js": "prettier --write --no-semi --single-quote --trailing-comma none --print-width 160",
    "gulpfile.babel.js": "prettier --write --no-semi --single-quote --trailing-comma none --print-width 160"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "spa -> <rootDir>/resource/apps/$1 || static -> <rootDir>/resource/base/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/resource/tests/**/*.test.+(ts|tsx|js)"
    ]
  },
  "scripts": {
    "setup:spa": "rm webpack/webpack.gulp.** && rm gulpfile.babel.js && rm .csscomb.json && rm resource/.gitkeep && rm -rf .git",
    "setup:static": "rm webpack/webpack.base.babel.js && rm webpack/webpack.dev.babel.js && rm webpack/webpack.pro.babel.js && rm .stylelintrc && rm resource/.gitkeep && rm .yarnclean && rm -rf .git && mkdir resource/json && mkdir resource/images && mkdir resource/favicons",
    "dev": "webpack-dev-server --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "jest": "jest --passWithNoTests",
    "remove-fasters-cache": "rm -rf node_modules/.cache/hard-source/",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(styled-components|@add-ignore-modules)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
